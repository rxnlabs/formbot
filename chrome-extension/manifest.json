{
  "name": "Form Filler",
  "description": "Fill any form",
  "version": "1.0",
  "options_page": "options.html",
  "permissions": [
    "activeTab", "storage"
  ],
  "background": {
    "scripts": ["scripts/moment.min.js", "background.js"],
    "persistent": true
  },
  "browser_action": {
    "default_title": "Fill forms on page"
  },
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "manifest_version": 2
}