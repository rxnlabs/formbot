{
  "name": "Form Filler",
  "description": "Fill any form",
  "version": "1.0",
  "options_page": "options.html",
  "permissions": [
    "activeTab", "storage"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Fill forms on page"
    // "default_popup": "popup.html"
  },
  "web_accessible_resources": ["fillForms.min.js"],
  "manifest_version": 2
}